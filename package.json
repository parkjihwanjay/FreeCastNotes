{
  "name": "freecastnotes",
  "version": "1.0.0",
  "description": "Open source Raycast Notes clone for macOS",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:swift": "concurrently \"vite\" \"cd swift-app && swift run\"",
    "build": "tsc && vite build",
    "build:swift": "tsc && vite build && cd swift-app && swift build -c release",
    "preview": "vite preview"
  },
  "license": "MIT",
  "dependencies": {
    "@tiptap/extension-code-block": "^3.19.0",
    "@tiptap/extension-horizontal-rule": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-task-item": "^3.19.0",
    "@tiptap/extension-task-list": "^3.19.0",
    "@tiptap/extension-underline": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
